{
  "version": "2.0.0",
  "command": "npm",
  "isShellCommand": true,
  "tasks": [
    {
      "taskName": "lint",
      "suppressTaskName": true,
      "args": [
        "run",
        "lint"
      ],
      "problemMatcher": {
        "base": "$tslint5",
        "fileLocation": "absolute"
      }
    }
  ]
}